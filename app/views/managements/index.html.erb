<p id="notice"><%= notice %></p>

<h1>Managements</h1>
<table>
  <thead>
    <tr>
      <th>name</th>
      <th>Email</th>
      <th>role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admins.each do |admin| %>
      <tr>
        <td><%= admin.name %></td>
        <td><%= admin.email %></td>
        <td><%= admin.role %></td>
        <td><%= link_to 'Show', management_path(admin)%></td>
        <td><%= link_to 'Edit', edit_management_path(admin) %></td>
        <td><%= link_to 'Destroy', management_path(admin), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

